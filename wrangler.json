{
	"name": "cw-dorama",
	"compatibility_date": "2025-10-08",
	"main": "./server.ts",
	"assets": {
		"directory": "./build/client"
	},
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "cw-dorama-db",
			"database_id": "local"
		}
	],
	"r2_buckets": [
		{
			"binding": "BUCKET",
			"bucket_name": "cw-dorama-media"
		}
	]
}